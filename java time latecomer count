import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		sc.useDelimiter(":|\\s+");
		int count=0;
		//boolean a=true;
		while(sc.hasNext())
		{
		    int h=sc.nextInt();
		    int m=sc.nextInt();
		    if(h==10)
		    {
		        if(m!=0)
		        count++;
		    }
		    else if(h>10)
		    count++;
		}System.out.println(count);

	}
}
