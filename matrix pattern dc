#include<stdio.h>
#include <stdlib.h>

int main()
{
int m,n,i,j,a[100][100];
scanf("%d%d",&m,&n);
for(i=0;i<m;i++)
for(j=0;j<n;j++)
{
   scanf("%d\t",&a[i][j]); 
}
for(j=0;j<n+2;j++)
{
    printf("*\t");
}
printf("\n");
for(i=0;i<m;i++)
{
printf("*\t");
for(j=0;j<n;j++)
{
    printf("%d\t",a[i][j]);
}
printf("*\t");
printf("\n");
}
for(j=0;j<n+2;j++)
{
    printf("*\t");
}
}
