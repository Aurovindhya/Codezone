#include<stdio.h>
#include <stdlib.h>

int main()
{
int n,i;
scanf("%d",&n);
for(i=1;i<=n;i++)
{
if(i%2!=0)
{
if(i!=n)
printf("%d/4\t",i);
else
printf("%d/4",i);
}
else if(i%4!=0)
{ 
    if(i!=n)
    printf("%d/2\t",i/2);
    else
    printf("%d/2",i/2);
}
else 
{
    if(i!=n)
    printf("%d\t",i/4);
    else
    printf("%d",i/4);
}
}
}
